{
  "name": "llm-orchestrator",
  "version": "0.5.1",
  "description": "Multi-agent system for autonomous task execution with LLMs. Support for local/remote LLM providers (Anthropic, OpenAI, Ollama, LM Studio, llama-server) with configurable base URLs.",
  "main": "dist/cli.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "orchestrator": "node dist/cli.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:unit": "node tests/test.mjs",
    "test:valkey": "node tests/test-valkey.mjs",
    "test:parallel": "node tests/test-parallel.mjs",
    "test:providers": "node tests/test-providers.mjs",
    "test:dependencies": "node tests/test-dependencies.mjs",
    "test:progress": "node tests/test-progress.mjs",
    "test:recovery": "node tests/test-recovery.mjs",
    "test:conditional": "node dist/tests/test-conditional-dependencies.js",
    "test:phase4": "npm run test:dependencies && npm run test:progress && npm run test:recovery",
    "test:phase5": "npm run test:conditional",
    "tui": "tsx src/tui/index.tsx"
  },
  "bin": {
    "llm-orchestrator": "./dist/cli.js"
  },
  "keywords": [
    "ai",
    "agents",
    "llm",
    "anthropic",
    "openai",
    "orchestrator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "chokidar": "^3.5.3",
    "commander": "^12.0.0",
    "ink": "^6.6.0",
    "ioredis": "^5.9.2",
    "openai": "^4.75.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "jest": "^29.7.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0"
  }
}
